  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "testlib": "ng test odata-observable-store",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "rmdir /s /q projects\\odata-observable-store\\docs && npx typedoc --tsconfig tsconfig.json --out projects/odata-observable-store/docs  --theme markdown projects/odata-observable-store/src",
    "dist": "npm run docs && ng build odata-observable-store --prod",
    "patch": "cd projects/odata-observable-store && npm version patch",
    "publish": "npm run patch && npm run dist && cd dist/odata-observable-store && npm publish",
    "view": "c: && \"\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe\"  --incognito --disable-web-security --disable-gpu --user-data-dir=%LOCALAPPDATA%\\Google\\chromeTemp http://localhost:4200"
  },
